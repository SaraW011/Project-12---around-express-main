{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": 0,
    "quotes": ["error", "double"],
    "implicit-arrow-linebreak": "off",
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}

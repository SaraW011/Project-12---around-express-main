{
  "extends": "airbnb-base",
  "rules": {
    /*eslint no-underscore-dangle: ["error", { "allow": ["foo_", "_bar"] }]*/
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": 0,
    "quotes": ["error", "double"],
    "implicit-arrow-linebreak": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
